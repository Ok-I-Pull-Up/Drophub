{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"installCommand": "npm install --legacy-peer-deps",
				"buildCommand": "npm run build"
			}
		}
	],
	"env": {
		"CI": "false",
		"ESLINT_NO_DEV_ERRORS": "true",
		"DISABLE_ESLINT_PLUGIN": "true"
	}
}
